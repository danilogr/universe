{
  "manifest_version": 2,

  "name": "One-click lolcats",
  "description": "This extension demonstrates a browser action with kittens.",
  "version": "1.1",

  "permissions": [
    "https://secure.flickr.com/"
    ],
  "browser_action": {
    "default_icon": "icon.png",
